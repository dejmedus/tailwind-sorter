{
  "name": "tailwind-sorter",
  "displayName": "Tailwind Sorter",
  "description": "Sort your tailwind classes in a predictable way",
  "icon": "icon.png",
  "version": "0.1.11",
  "pricing": "Free",
  "engines": {
    "vscode": "^1.85.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dejmedus/tailwind-sorter"
  },
  "bugs": {
    "url": "https://github.com/dejmedus/tailwind-sorter/issues"
  },
  "publisher": "dejmedus",
  "license": "MIT",
  "keywords": [
    "tailwind",
    "sorting",
    "TailwindCSS"
  ],
  "categories": [
    "Formatters",
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "configuration": {
      "title": "Tailwind Sorter",
      "properties": {
        "tailwindSorter.customPrefixes": {
          "type": "array",
          "items": {
            "type": "string",
            "minLength": 1
          },
          "default": [
            "twMerge(",
            "clsx(",
            "cva("
          ],
          "description": "Custom prefixes to identify tailwind class strings. Example: clsx( or tw="
        },
        "tailwindSorter.categoryOrder": {
          "type": "object",
          "properties": {
            "sortOrder": {
              "type": "array",
              "minItems": 1,
              "items": {
                "type": "string",
                "minLength": 1
              }
            }
          },
          "additionalProperties": false,
          "default": {
            "sortOrder": [
              "box",
              "flex",
              "grid",
              "bg-visual",
              "margins",
              "borders",
              "sizing",
              "typography",
              "transformations",
              "other"
            ]
          },
          "description": "Sort order of Tailwind categories"
        },
        "tailwindSorter.pseudoClassesOrder": {
          "type": "object",
          "properties": {
            "sortOrder": {
              "type": "array",
              "minItems": 1,
              "items": {
                "type": "string",
                "minLength": 1
              }
            }

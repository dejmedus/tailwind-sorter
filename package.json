          }
        },
        "tailwindSorter.categories": {
          "type": "object",
          "propertyNames": {
            "type": "string",
            "minLength": 1
          },
          "patternProperties": {
            ".*": {
              "type": "array",
              "minItems": 1,
              "items": {
                "type": "string",
                "minLength": 1
              }
            }
